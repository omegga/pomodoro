{
  "name": "pomodoro",
  "version": "1.0.0",
  "description": "Pomodoro Clock",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack-dev-server --config webpack.dev.js",
    "build:prod": "webpack --config webpack.prod.js",
